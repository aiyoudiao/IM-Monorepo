{
  "name": "im-client",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "cross-env UMI_ENV=prod max build",
    "dev": "npm run start:dev",
    "postinstall": "max setup && patch-package",
    "jest": "jest",
    "lint": "npm run lint:js && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier -c --write \"**/**.{js,jsx,tsx,ts,less,md,json}\"",
    "preview": "npm run build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start:mock": "cross-env UMI_ENV=develop max dev",
    "start": "cross-env MOCK=none REACT_APP_ENV=local UMI_ENV=develop max dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=develop max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=test max dev",
    "start:staging": "cross-env REACT_APP_ENV=staging MOCK=none UMI_ENV=staging max dev",
    "start:prod": "cross-env REACT_APP_ENV=prod MOCK=none UMI_ENV=staging max dev",
    "sync-arms": "node scripts/arms-oss-api.js",
    "test": "jest",
    "test:coverage": "npm run jest -- --coverage",
    "test:update": "npm run jest -- -u",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@ant-design/pro-components": "^2.6.48",
    "@ant-design/x": "^1.0.5",
    "@iconify-json/ant-design": "^1.2.5",
    "@umijs/route-utils": "^2.2.2",
    "antd": "^5.13.2",
    "antd-style": "^3.6.1",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "decimal.js": "^10.4.3",
    "delta": "^3.0.5",
    "dompurify": "^3.2.3",
    "history": "5.3.0",
    "html-react-parser": "^5.2.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "omit.js": "^2.0.2",
    "querystring": "^0.2.1",
    "rc-menu": "^9.12.4",
    "rc-util": "^5.38.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@ant-design/cssinjs-utils": "1.1.3",
    "@ant-design/pro-cli": "^3.3.0",
    "@testing-library/react": "^13.4.0",
    "@types/ali-oss": "^6.16.11",
    "@types/classnames": "^2.3.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/history": "^4.7.11",
    "@types/jest": "^29.5.11",
    "@types/lodash-es": "^4.17.12",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.1.1",
    "@umijs/max": "^4.1.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.4",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^10.5.4",
    "patch-package": "^8.0.0",
    "prettier": "^2.8.8",
    "react-dev-inspector": "^1.9.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "umi-plugin-click-to-code": "^1.1.1",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
